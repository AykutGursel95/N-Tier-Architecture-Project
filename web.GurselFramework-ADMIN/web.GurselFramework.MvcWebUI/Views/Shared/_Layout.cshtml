    <script src="~/Assets/Plugins/Scripts/jquery.vmap.usa.js"></script>
    <script src="~/Assets/Plugins/Scripts/jquery.vmap.sampledata.js"></script>

    <script src="~/Assets/Plugins/Scripts/app.min.js"></script>
    <script src="~/Assets/Plugins/Scripts/dashboard.min.js"></script>
    <script src="~/Assets/Plugins/Scripts/layout.min.js"></script>
    <script src="~/Assets/Plugins/Scripts/quick-sidebar.min.js"></script>
    <script src="~/Assets/Plugins/Scripts/quick-nav.min.js"></script>

    <script>
        $(document).ready(function () {
            $('#clickmewow').click(function () {
                $('#radio1003').attr('checked', 'checked');
            });
        })</script>
</body>

</html>